<COMPONENT>
	<HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.1"
    STATUS="DEVELOPMENT"
 
    COMPATIBILITY="ALL"
    AUTHOR="Stefan Scheffler"
    RELEASE_DATE="20070525A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

	<FAMILIES>
		<FAMILY>semanticWeb/Jena</FAMILY>
	</FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER
	      NAME="JENA_CONNECTION"
	      FIELD="COMPONENT_LIST"
	      REQUIRED="true"
	      FILTER="tJenaConnection"
	      NUM_ROW="1"
	      DYNAMIC_SETTINGS="true"
	      GROUP="db_type"

	    />
	    
		
		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" NUM_ROW="5" READONLY="false" />
		
		<PARAMETER NAME="USE_GLOBALNAMESPACEMAP_CHECK" FIELD="CHECK" REQUIRED="true" NUM_ROW="5" >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="USE_QUERY" FIELD="RADIO" REQUIRED="true" NUM_ROW="7" GROUP="SERVICE">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="USE_OFFSET_CHECK" FIELD="CHECK" REQUIRED="true" NUM_ROW="7" GROUP="SERVICE" SHOW_IF="USE_QUERY == 'true'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="OFFSET" FIELD="MEMO" REQUIRED="true" SHOW_IF="USE_OFFSET_CHECK == 'true'" NB_LINES="1" NUM_ROW="8" GROUP="SERVICE">
	    	<DEFAULT>50000</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="USE_UPDATE" FIELD="RADIO" REQUIRED="true" NUM_ROW="9" GROUP="SERVICE" >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="QUERY" FIELD="MEMO_SQL" REQUIRED="true" NB_LINES="10" NUM_ROW="20">
	    	<DEFAULT>"Select ?x ?y ?z ?graph {graph ?graph{?x ?y ?z}}"</DEFAULT>
		</PARAMETER>

	</PARAMETERS>
	
	<ADVANCED_PARAMETERS>

		<PARAMETER NAME="LOGGING" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="5">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<!--<IMPORT NAME="fuseki" MODULE="fuseki-server.jar" REQUIRED="true" />
			<IMPORT NAME="virt_jena" MODULE="virt_jena_own.jar" REQUIRED="true" />
			<IMPORT NAME="jdbc_jena" MODULE="virtjdbc3.jar" REQUIRED="true" />-->
			<IMPORT NAME="jena_talend_connector" MODULE="talend_jena_connector.jar" REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>


	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
	</RETURNS>
</COMPONENT>
